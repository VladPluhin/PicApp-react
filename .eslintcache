;\r\n  }\r\n};\r\n\r\nexport default SectionMain;\r\n","D:\\study\\PictureApp-react\\src\\components\\SectionLikedPost\\SectionLikedPost.js",[],"D:\\study\\PictureApp-react\\src\\components\\SectionAbout\\SectionAbout.js",[],"D:\\study\\PictureApp-react\\src\\components\\Spiner\\Spiner.js",[],"D:\\study\\PictureApp-react\\src\\state\\state.js",[],"D:\\study\\PictureApp-react\\src\\components\\CardRow\\CardRow.js",["96","97"],"import React, { useContext } from \"react\";\r\nimport classes from \"./cardRow.module.scss\";\r\nimport Card from \"../Card/Card\";\r\nimport { LikesContext } from \"../../context/context\";\r\n\r\nconst CardRow = (props) => {\r\n  const { likePostData } = useContext(LikesContext);\r\n\r\n  if (props.likesRow == false) {\r\n    return (\r\n      <div className={classes.cardRow}>\r\n        {props.data.map((card, id) => (\r\n          <div key={id + 1} className={classes.cardCol}>\r\n            <Card likesRow={false} card={card} />\r\n          </div>\r\n        ))}\r\n\r\n      </div>\r\n    );\r\n  } else {\r\n    if (likePostData.length == 0) {\r\n      return <h1>No posts chosen...</h1>;\r\n    } else {\r\n      return (\r\n        <div className={classes.cardRow}>\r\n          {likePostData.map((card) => (\r\n            <div key={card.id} className={classes.cardCol}>\r\n              <Card likesRow={true} card={card} />\r\n            </div>\r\n          ))}\r\n        </div>\r\n      );\r\n    }\r\n  }\r\n};\r\n\r\nexport default CardRow;\r\n","D:\\study\\PictureApp-react\\src\\components\\Card\\Card.js",["98","99","100"],"import React, { useContext, useState , useRef} from \"react\";\r\nimport classes from \"./card.module.scss\";\r\nimport { LikesContext } from \"../../context/context\";\r\nimport Popup from \"../Popup/Popup\";\r\nimport { CSSTransition } from 'react-transition-group';\r\n\r\nconst Card = (props) => {\r\n  const card = props.card;\r\n  const { likePostData, setLikedPost } = useContext(LikesContext);\r\n  const [hovered, setHovered] = useState(false);\r\n  const hoverOff = useRef();\r\n  const hoverOn = useRef();\r\n  const gethoverOff=()=> {\r\n    setHovered(false)\r\n    return  ()=> {hoverOff.current.removeEventLisener(\"mouseleave\", gethoverOff)}\r\n  }\r\n\r\n  const gethoverOn=()=> {\r\n    setHovered(true)\r\n    return  ()=> {hoverOn.current.removeEventLisener(\"mouseenter\", gethoverOn)};\r\n  }\r\n\r\n  const getLike = (card, arr) => {\r\n    let selectPhoto = card ;\r\n    let newArr = arr;\r\n    const even = (item) => item.id == selectPhoto.id;\r\n    if (newArr.length === 0) {\r\n      newArr.push(selectPhoto);\r\n      return newArr;\r\n    } else if (!newArr.some(even)) {\r\n      newArr.push(selectPhoto);\r\n      return newArr;\r\n    }\r\n    return newArr;\r\n  };\r\n\r\n  const deletedPost = (card, arr) => {\r\n    var filtered = arr.filter(function (el) {\r\n      return el.id != card.id;\r\n    });\r\n    return filtered;\r\n  };\r\n\r\n\r\n  return (\r\n    <div className={classes.card}  ref={hoverOff} onMouseLeave={gethoverOff}>\r\n      <div className={classes.cardImg}>\r\n        <img className=\"img\" src={card.urls.regular} />\r\n      </div>\r\n      <div className={classes.cardBody}>\r\n        {props.likesRow ?\r\n          <button className={classes.btnDelet} onClick={() => setLikedPost(deletedPost(props.card, likePostData))}>\r\n            &#9747;\r\n          </button>\r\n         : <button className={classes.btnLike} onClick={() => setLikedPost(getLike(props.card, likePostData))} >\r\n            &#10084;\r\n          </button>\r\n        }\r\n        <time className={classes.date} dateTime={props.card.created_at ? props.card.created_at.slice(0, 10) : \"\"}>\r\n          {props.card.created_at ? props.card.created_at.slice(0, 10) : \"\"}\r\n        </time>\r\n        <div className={classes.description}>\r\n          {props.card.description ? <p> {props.card.description} </p> : \" \"}\r\n        </div>\r\n        <span className={classes.title}  ref={ hoverOn} onMouseOver={gethoverOn}>\r\n          <span>Author:</span>\r\n          {card.name ? card.name : card.user.username}\r\n        </span>\r\n      </div>\r\n      <CSSTransition\r\n        in={hovered}\r\n        timeout={300}\r\n        classNames=\"popup\"\r\n        unmountOnExit>\r\n          <Popup user= {props.card.user}/>\r\n      </CSSTransition>\r\n\r\n    </div>\r\n  );\r\n};\r\n// Card.propTypes={\r\n// \tphoto:PropTypes.shape({\r\n// \t\t urls: PropTypes.shape({\r\n// \t\t\tregular: PropTypes.string\r\n// \t\t }),\r\n// \t\tuser: PropTypes.shape({\r\n// \t\t\tname:PropTypes.string\r\n// \t\t}),\r\n// \t})\r\n// }\r\nexport default Card;\r\n","D:\\study\\PictureApp-react\\src\\hooks\\useObserver.js",[],"D:\\study\\PictureApp-react\\src\\components\\Popup\\Popup.js",[],"D:\\study\\PictureApp-react\\src\\components\\Sorting\\Sorting.js",["101"],"import React, {useState} from \"react\";\r\nimport classes from \"./sorting.module.scss\";\r\nimport { CSSTransition } from 'react-transition-group';\r\nimport Select from \"../../UI/Select\";\r\nimport Search from \"../../UI/Search\";\r\nimport Range from \"../../UI/Range\";\r\nimport BtnPrimary from '../../UI/BtnPrimary';\r\n\r\n// import PropTypes from 'prop-types';\r\n\r\nconst Sorting = (props) => {\r\n  const [showed, setShowedFilter] = useState(false);\r\n  const [searchQuery, setSearchQuery]= useState('');\r\n  const [colorValue, setColorValue]  = useState(['black_and_white', 'black', 'white', 'yellow', 'orange', 'red', 'purple', 'magenta', 'green', 'teal', 'blue'])\r\n\r\n  const getShowedFilter = ()=> {\r\n    return showed ? setShowedFilter(false) :  setShowedFilter(true)\r\n  }\r\n\r\n  return(\r\n      <div className={classes.sorting} >\r\n          <button className={classes.sortingclose}\r\n            onClick={()=>{getShowedFilter()}}>\r\n              <span></span>\r\n              <span></span>\r\n              <span></span>\r\n          </button>\r\n          <CSSTransition\r\n  